<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="android:textSize">14sp</item> <!-- Adjust the text size as needed -->
        <item name="android:fontFamily">@font/avenir_book</item> <!-- Replace "your_font_family" with your desired font family -->
    </style>

    <style name="profile_round">
        <item name="cornerSize">50%</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="bottom_nav_text">
        <item name="android:fontFamily">@font/avenir_black</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ToolbarStyle" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:fontFamily">@font/avenir_black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/ace_secondary_variant</item>
    </style>

    <style name="outlined_til">
        <item name="boxBackgroundColor">@null</item>
        <item name="boxBackgroundMode">outline</item>
        <item name="boxStrokeWidth">2dp</item>
        <item name="boxCornerRadiusBottomEnd">15dp</item>
        <item name="boxCornerRadiusBottomStart">15dp</item>
        <item name="boxCornerRadiusTopEnd">15dp</item>
        <item name="boxCornerRadiusTopStart">15dp</item>
        <item name="errorTextColor">@color/ace_error</item>
        <item name="boxStrokeErrorColor">@color/ace_error</item>
        <item name="errorIconTint">@color/ace_error</item>
    </style>

    <style name="outlined_tiet">
        <item name="android:textColor">@color/ace_secondary_variant</item>
        <item name="android:paddingBottom">17dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="outlined_tiet2">
        <item name="android:textColor">@color/ace_secondary_variant</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingTop">18dp</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="button_primary">
        <item name="android:layout_height">70dp</item>
        <item name="iconSize">30dp</item>
        <item name="iconTint">@null</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">15dp</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="button_tertiary">
        <item name="android:layout_height">60dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">15dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="strokeWidth">2dp</item>
        <item name="strokeColor">@color/ace_orange</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="button_primary2">
        <item name="android:layout_height">65dp</item>
        <item name="iconSize">28dp</item>
        <item name="iconTint">@null</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">15dp</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="button_secondary">
        <item name="android:layout_height">70dp</item>
        <item name="iconSize">30dp</item>
        <item name="iconTint">@null</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="strokeWidth">3dp</item>
        <item name="strokeColor">@color/ace_secondary_variant</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:textColor">@color/ace_secondary_variant</item>
        <item name="cornerRadius">15dp</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="button_filter">
        <item name="android:layout_height">50dp</item>
        <item name="android:backgroundTint">@color/ace_background</item>
        <item name="strokeWidth">2dp</item>
        <item name="strokeColor">@color/ace_secondary_variant</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/ace_secondary_variant</item>
        <item name="cornerRadius">10dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginEnd">10dp</item>
        <item name="android:paddingStart">15dp</item>
        <item name="android:paddingEnd">15dp</item>
    </style>

    <style name="button_settings" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_height">60dp</item>
        <item name="iconSize">25dp</item>
        <item name="iconTint">@color/ace_secondary_variant</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:textColor">@color/ace_secondary_variant</item>
        <item name="cornerRadius">0dp</item>
        <item name="android:textSize">15sp</item>
        <item name="elevation">0dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="iconPadding">20dp</item>
    </style>

    <style name="small_secondary_text">
        <item name="android:textColor">@color/ace_secondary_variant</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="head_secondary_text">
    <item name="android:textColor">@color/black</item>
    <item name="android:paddingTop">10dp</item>
    <item name="android:paddingBottom">10dp</item>
    <item name="android:textSize">20sp</item>
</style>
</resources>